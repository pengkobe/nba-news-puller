{
    "manifest_version": 2,
    "name": "NBA-NEWS-PULLER",
    "description": "This extension build to pull nba news from nba.qq.com",
    "version": "1.0",
    "icons": {
        "16": "images/nba16.png",
        "48": "images/nba48.png",
        "128": "images/nba128.png"
    },
    "browser_action": {
        "default_icon": "images/nba48.png",
        "default_popup": "popup.html",
        "default_title": "yipeng:Click to pull!"
    },
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/"
    ]
}