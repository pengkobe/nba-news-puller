{
  "manifest_version": 2,

  "name": "NBA-NEWS-PULLER",
  "description": "This extension build to pull nba news from nba.qq.com",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click yipeng to pull!"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}